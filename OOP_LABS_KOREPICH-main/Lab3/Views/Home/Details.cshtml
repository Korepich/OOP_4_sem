@model Lab3.Models.Product
@{
    ViewBag.Title = Model.name;
}
<h2>Продукт: @Model.name</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Идентификатор</dt>
        <dd>@Model.id</dd>
 
        <dt>Количество</dt>
        <dd>@Model.count</dd>

         <dt>Цена</dt>
        <dd>@Model.price</dd>
    </dl>
</div>